{
  "name": "arraypress/wp-string-utils",
  "version": "1.0.0",
  "description": "Essential string manipulation utilities for WordPress development",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-string-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\StringUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-string-utils/issues",
    "source": "https://github.com/arraypress/wp-string-utils"
  },
  "keywords": [
    "wordpress",
    "strings",
    "text-processing",
    "wp",
    "arraypress",
    "string-manipulation",
    "validation",
    "case-conversion",
    "content-processing",
    "utilities"
  ]
}